{
  "backend": {
    "consul": {
      "address": "127.0.0.1:8500",
      "path": "<%= path %>"
    }
  },
   "listener": {
    "tcp": {
      "address": "127.0.0.1:8200",
      "tls_disable": 1
    }
  },
  "listener": {
    "tcp": {
      "address": "<%= ipaddress %>:8200",
      "tls_cert_file": "/etc/ssl/certs/<%= tls_cert_file %>",
      "tls_key_file": "/etc/ssl/certs/<%= tls_key_file %>"
    }
  }
}
